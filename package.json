{
  "name": "home-server-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "local:serve-interactive": "run-s lint && ng serve --open --proxy-config proxy.conf.json",
    "ci:build": "run-s lint test-with-coverage build",
    "ci:release": "run-s test-no-watch-no-coverage build create-zip",
    "ng": "ng",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf coverage",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "test-interactive": "ng test",
    "test-no-watch-no-coverage": "ng test --watch=false --browsers=ChromeHeadless",
    "test-with-coverage": "ng test --code-coverage --watch=false --browsers=ChromeHeadless",
    "build": "ng build --configuration production",
    "create-zip": "cd dist && zip -r home-frontend.zip . && cd ..",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^16.1.1",
    "@angular/animations": "^16.1.2",
    "@angular/cdk": "^16.1.2",
    "@angular/common": "^16.1.2",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.2",
    "@angular/forms": "^16.1.2",
    "@angular/localize": "^16.1.2",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.2",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@ng-bootstrap/ng-bootstrap": "^15.1.1",
    "@popperjs/core": "^2.11.8",
    "@stomp/ng2-stompjs": "^8.0.0",
    "@types/c3": "^0.4.54",
    "@types/chroma-js": "^2.4.0",
    "@types/popper.js": "^1.11.0",
    "bootstrap": "^5.3.1",
    "c3": "^0.4.24",
    "chroma-js": "^2.4.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.7.0",
    "lodash-es": "^4.17.21",
    "ng2-date-picker": "^16.0.0",
    "ngx-spinner": "^16.0.2",
    "popper.js": "^1.16.1",
    "rxjs": "^7.8.1",
    "sockjs-client": "^1.6.1",
    "stompjs": "^2.3.3",
    "tslib": "^2.5.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^16.0.3",
    "@angular-eslint/eslint-plugin": "^16.0.3",
    "@angular-eslint/eslint-plugin-template": "^16.0.3",
    "@angular-eslint/schematics": "^16.0.3",
    "@angular-eslint/template-parser": "^16.0.3",
    "@angular/cli": "^16.1.1",
    "@angular/compiler-cli": "^16.1.1",
    "@angular/language-service": "^16.1.2",
    "@types/jasmine": "~4.3.4",
    "@types/jasminewd2": "~2.0.10",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.5.0",
    "@types/sockjs-client": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "dayjs": "^1.11.9",
    "eslint": "^8.47.0",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.6"
  }
}
