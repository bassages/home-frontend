{
  "name": "home-server-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "local:serve-interactive": "run-s lint && ng serve --open --proxy-config proxy.conf.json",
    "ci:build": "run-s lint test-with-coverage build",
    "ci:release": "run-s test-without-coverage build create-zip",
    "ci:test-e2e": "start-server-and-test serve http://localhost:4200 cypress:run",
    "ng": "ng",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf coverage",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "lint": "ng lint",
    "test-without-coverage": "ng test --watch=false --browsers=ChromeHeadless",
    "test-with-coverage": "ng test --code-coverage  --watch=false --browsers=ChromeHeadless",
    "build": "ng build --configuration production",
    "create-zip": "cd dist && zip -r home-frontend.zip . && cd ..",
    "reinstall": "npm run clean && npm install",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "rebuild": "npm run clean && npm install && npm run build",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/animations": "13.3.4",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "13.3.4",
    "@angular/compiler": "13.3.4",
    "@angular/core": "13.3.4",
    "@angular/forms": "13.3.4",
    "@angular/localize": "^13.3.4",
    "@angular/platform-browser": "13.3.4",
    "@angular/platform-browser-dynamic": "13.3.4",
    "@angular/router": "13.3.4",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@ng-bootstrap/ng-bootstrap": "^12.0.1",
    "@popperjs/core": "^2.11.4",
    "@stomp/ng2-stompjs": "^8.0.0",
    "@types/c3": "^0.4.54",
    "@types/chroma-js": "^2.1.3",
    "@types/popper.js": "^1.11.0",
    "bootstrap": "^5.0.0",
    "c3": "^0.4.24",
    "chroma-js": "^2.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "lodash-es": "^4.17.21",
    "ng2-date-picker": "^13.1.1",
    "ngx-spinner": "^13.1.1",
    "popper.js": "^1.16.1",
    "rxjs": "^7.4",
    "sockjs-client": "^1.5.1",
    "stompjs": "^2.3.3",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.3",
    "@angular/compiler-cli": "13.3.4",
    "@angular/language-service": "13.3.4",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.25",
    "@types/sockjs-client": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "5.20.0",
